<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.TilePane?>
<?import java.lang.String?>
<TilePane fx:id="boardSquare" alignment="CENTER" hgap="5.0" prefColumns="3" prefRows="3" styleClass="gameBoardSquare"
          vgap="5.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fx:id="ammoCardSlot" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
            <styleClass>
                <String fx:value="gameBoardItem"/>
                <String fx:value="ammoCard"/>
            </styleClass>
        </ImageView>
    </children>
</TilePane>
