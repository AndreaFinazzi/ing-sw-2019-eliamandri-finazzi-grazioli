<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="it.polimi.se.eliafinazzigrazioli.adrenaline.client.GUI.controllers.CommandGUIController"
            prefHeight="400.0" prefWidth="600.0">
    <HBox fx:id="arrows" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <children>
            <GridPane alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <children>
                    <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="2">
                        <children>
                            <Button alignment="CENTER" mnemonicParsing="false" onMouseClicked="#arrowDOWNPressed"
                                    text="DOWN" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                        </children>
                    </AnchorPane>
                    <AnchorPane GridPane.columnIndex="1">
                        <children>
                            <Button mnemonicParsing="false" onMouseClicked="#arrowUPPressed" text="UP"
                                    AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                        </children>
                    </AnchorPane>
                    <AnchorPane GridPane.columnIndex="2" GridPane.rowIndex="1">
                        <children>
                            <Button mnemonicParsing="false" onMouseClicked="#arrowRIGHTPressed" text="RIGHT"
                                    AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                        </children>
                    </AnchorPane>
                    <AnchorPane GridPane.rowIndex="1">
                        <children>
                            <Button mnemonicParsing="false" onMouseClicked="#arrowLEFTPressed" text="LEFT"
                                    AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                        </children>
                    </AnchorPane>
                    <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="1">
                        <children>
                            <Button mnemonicParsing="false" onMouseClicked="#pathCONFIRMButtonPressed" text="CONFIRM"
                                    AnchorPane.bottomAnchor="12.0" AnchorPane.leftAnchor="12.0"
                                    AnchorPane.rightAnchor="12.0" AnchorPane.topAnchor="12.0"/>
                        </children>
                    </AnchorPane>
                </children>
            </GridPane>
        </children>
    </HBox>
</AnchorPane>
