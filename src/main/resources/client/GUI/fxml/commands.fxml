<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.TextFlow?>
<?import java.lang.*?>
<GridPane fx:id="commandsGridPane" maxHeight="396.0" maxWidth="935.0" minHeight="396.0" minWidth="935.0"
          prefHeight="396.0" prefWidth="935.0" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8.0.171"
          xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="it.polimi.se.eliafinazzigrazioli.adrenaline.client.GUI.controllers.CommandGUIController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="758.0" minWidth="10.0" percentWidth="72.0" prefWidth="758.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="608.0" minWidth="10.0" percentWidth="28.0" prefWidth="155.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="274.0" minHeight="10.0" prefHeight="239.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="193.0" minHeight="10.0" percentHeight="38.0" prefHeight="155.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <AnchorPane prefHeight="246.0" prefWidth="264.0" GridPane.columnIndex="1">
            <children>
                <TextFlow layoutX="28.0" layoutY="23.0" prefHeight="200.0" prefWidth="200.0"
                          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                          AnchorPane.topAnchor="0.0"/>
            </children>
        </AnchorPane>
        <AnchorPane fx:id="myPlayerBoardAnchorPane" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
            <!--Here we'll have my player board-->
        </AnchorPane>
        <AnchorPane prefHeight="200.0" prefWidth="200.0" styleClass="cardSlots">
            <children>
                <HBox id="weaponCardSlots" fx:id="myWeaponCardSlots" alignment="CENTER" layoutX="105.0" layoutY="73.0"
                      prefHeight="226.0" prefWidth="326.0" spacing="3.0" AnchorPane.bottomAnchor="10.0"
                      AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="300.0" AnchorPane.topAnchor="0.0">
                    <children>
                        <Button alignment="CENTER" mnemonicParsing="false">
                            <styleClass>
                                <String fx:value="myWeaponCard"/>
                                <String fx:value="weaponCard"/>
                            </styleClass>
                        </Button>
                        <Button alignment="CENTER" mnemonicParsing="false">
                            <styleClass>
                                <String fx:value="myWeaponCard"/>
                                <String fx:value="weaponCard"/>
                            </styleClass>
                        </Button>
                        <Button alignment="CENTER" mnemonicParsing="false">
                            <styleClass>
                                <String fx:value="myWeaponCard"/>
                                <String fx:value="weaponCard"/>
                            </styleClass>
                        </Button>
                    </children>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                </HBox>
                <HBox id="powerUpSlots" fx:id="myPowerUpSlots" alignment="CENTER" layoutX="360.0" layoutY="25.0"
                      prefHeight="196.0" prefWidth="232.0" spacing="3.0" AnchorPane.bottomAnchor="10.0"
                      AnchorPane.leftAnchor="373.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <children>
                        <Button alignment="CENTER" mnemonicParsing="false" onAction="#myPowerUpSelected">
                            <styleClass>
                                <String fx:value="powerUpCard"/>
                                <String fx:value="myPowerUp"/>
                            </styleClass>
                        </Button>
                        <Button alignment="CENTER" mnemonicParsing="false" onAction="#myPowerUpSelected">
                            <styleClass>
                                <String fx:value="powerUpCard"/>
                                <String fx:value="myPowerUp"/>
                            </styleClass>
                        </Button>
                        <Button alignment="CENTER" mnemonicParsing="false" onAction="#myPowerUpSelected">
                            <styleClass>
                                <String fx:value="powerUpCard"/>
                                <String fx:value="myPowerUp"/>
                            </styleClass>
                        </Button>
                    </children>
                </HBox>
            </children>
        </AnchorPane>
        <StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <children>
                <GridPane fx:id="arrowsGridPane" alignment="CENTER" disable="true" nodeOrientation="LEFT_TO_RIGHT"
                          visible="false">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="2">
                            <children>
                                <Button fx:id="arrowDOWN" alignment="CENTER" mnemonicParsing="false"
                                        onMouseClicked="#arrowDOWNPressed" text="DOWN" AnchorPane.bottomAnchor="0.0"
                                        AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                        AnchorPane.topAnchor="0.0"/>
                            </children>
                        </AnchorPane>
                        <AnchorPane GridPane.columnIndex="1">
                            <children>
                                <Button fx:id="arrowUP" mnemonicParsing="false" onMouseClicked="#arrowUPPressed"
                                        text="UP" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                        AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                            </children>
                        </AnchorPane>
                        <AnchorPane GridPane.columnIndex="2" GridPane.rowIndex="1">
                            <children>
                                <Button fx:id="arrowRIGHT" mnemonicParsing="false" onMouseClicked="#arrowRIGHTPressed"
                                        text="RIGHT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                        AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                            </children>
                        </AnchorPane>
                        <AnchorPane GridPane.rowIndex="1">
                            <children>
                                <Button fx:id="arrowLEFT" mnemonicParsing="false" onMouseClicked="#arrowLEFTPressed"
                                        text="LEFT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                        AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                            </children>
                        </AnchorPane>
                        <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <children>
                                <Button fx:id="arrowSTOP" mnemonicParsing="false"
                                        onMouseClicked="#arrowSTOPButtonPressed" text="CONFIRM"
                                        AnchorPane.bottomAnchor="12.0" AnchorPane.leftAnchor="12.0"
                                        AnchorPane.rightAnchor="12.0" AnchorPane.topAnchor="12.0"/>
                            </children>
                        </AnchorPane>
                    </children>
                </GridPane>
                <FlowPane fx:id="actionsFlowPane" alignment="CENTER" columnHalignment="CENTER" hgap="10.0"
                          prefHeight="200.0" prefWidth="200.0" vgap="10.0">
                    <StackPane.margin>
                        <Insets/>
                    </StackPane.margin>
                </FlowPane>
            </children>
        </StackPane>
    </children>
</GridPane>
